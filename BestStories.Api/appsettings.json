{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HackerNewsClient": {
    "BaseUri": "https://hacker-news.firebaseio.com/",
    "MaxDegreeOfParallelism": 25,
    "ConcurrentRequestsLimit": 500,
    "ConcurrentRequestQueueLimit": 1000
  },
  "Cache": {
    "SlidingExpiration": "00:02:00",
    "PrefetchInterval": "00:02:00"
  }
}
